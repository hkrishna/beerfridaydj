<section id="users">
  <div class="page-header">
    <h3>Users</h3>
  </div>
  <div class="row">
    <div class="span16 columns">
      <ul class="unstyled">
      <%- @users.each do |user| %>
      <li>
        <h5 class="user-name"><%= link_to(user.display_name, user_path(user)) %>
          <small class="video-count">[<%= user.videos.size %> videos]</small>
        </h5>
      </li>
      <%- end %>
      </ul>
    </div>
  </div>
</section>
